    <!--HEADER-->
    <div id="header-wrap">
      <header>
        <%= image_tag("images/estructura-eventos/header-image.png") , alt="header-img" %>
        <article>
          <p class="pre-title">Prepare for</p>
          <h1>Animecon <span>2015</span></h1>
          <h6>Los Angeles CA - February 17th, 18th and 19th</h6>
          <a href="#schedule" class="button pink">Full Schedule</a>
          <a href="#tickets" class="button ruby">Buy Tickets</a>
          <div id="countdown" class="xmcountdown">
            <div class="days" style="float: left">
              <canvas width="130" height="130"></canvas>
              <p style="width: 130px; margin-left: -65px">
                <span class="value">0-1909</span><br /><span class="tag"
                  >days</span
                >
              </p>
            </div>
            <div class="hours" style="float: left">
              <canvas width="130" height="130"></canvas>
              <p style="width: 130px; margin-left: -65px">
                <span class="value">0-2</span><br /><span class="tag"
                  >hours</span
                >
              </p>
            </div>
            <div class="minutes" style="float: left">
              <canvas width="130" height="130"></canvas>
              <p style="width: 130px; margin-left: -65px">
                <span class="value">0-40</span><br /><span class="tag"
                  >minutes</span
                >
              </p>
            </div>
            <div class="seconds" style="float: left">
              <canvas width="130" height="130"></canvas>
              <p style="width: 130px; margin-left: -65px">
                <span class="value">0-33</span><br /><span class="tag"
                  >seconds</span
                >
              </p>
            </div>
          </div>
        </article>
      </header>
    </div>
